{
    "name": "eliberty/cmf-core-bundle",
    "type": "symfony-bundle",
    "description": "Symfony CMF Core Bundle",
    "keywords": [
        "Symfony CMF"
    ],
    "homepage": "http://cmf.symfony.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Symfony CMF Community",
            "homepage": "https://github.com/symfony-cmf/CoreBundle/contributors"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://e-liberty.repo.repman.rpstack.com"
        }
    ],
    "require": {
        "php": ">=7.4|^8.0",
        "symfony/framework-bundle": "^3.4 || ^4.4 || ^5.0",
        "symfony/security-core": "^3.4 || ^4.4 || ^5.0",
        "twig/twig": "^2.0 || ^3.0"
    },
    "require-dev": {
        "symfony/security-bundle": "^3.4 || ^4.4 || ^5.0",
        "symfony/phpunit-bridge": "^3.3 || ^4.0",
        "mockery/mockery": "^0.9.4",
        "symfony-cmf/routing-bundle": "^2.1.0",
        "symfony-cmf/testing": "^3.0.0",
        "doctrine/dbal": "^2.10.0",
        "doctrine/phpcr-odm": "^1.4|^2.0 ",
        "jackalope/jackalope": "^1.4",
        "jackalope/jackalope-doctrine-dbal": "^1.7",
        "symfony/browser-kit": "^4.4 || ^5.0",
        "symfony/templating": "^4.4 || ^5.0",
        "symfony/form": "^4.4 || ^5.0",
        "symfony/translation": "^4.4 || ^5.0",
        "symfony/validator": "^4.4 || ^5.0",
        "symfony/asset": "^4.4 || ^5.0",
        "rector/rector": "^0.15.21",
        "squizlabs/php_codesniffer": "^3.7",
        "phpcompatibility/php-compatibility": "^9.3"
    },
    "suggest": {
        "symfony/twig-bundle": "To get access to the CMF twig extension (^2.8 || ^3.3 || ^4.0)",
        "doctrine/phpcr-bundle": "To be able to use the CMF twig extension (^1.0)",
        "doctrine/phpcr-odm": "To be able to use the CMF twig extension (^1.0)",
        "symfony/security-bundle": "To be able to use the publish workflow system (^2.8 || ^3.3 || ^4.0)",
        "symfony-cmf/routing": "To be able to use the CMF twig extension functions cmf_prev_linkable/cmf_next_linkable (^2.1)",
        "symfony-cmf/routing-bundle": "To be able to enable the publish_workflow_listener (^2.1)",
        "symfony-cmf/sonata-admin-integration-bundle": "To provide an admin interface for the PHPCR ODM documents."
    },
    "autoload": {
        "psr-4": {
            "Symfony\\Cmf\\Bundle\\CoreBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symfony\\Cmf\\Bundle\\CoreBundle\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.2-dev"
        }
    },
    "scripts": {
        "lint:rector": "./vendor/bin/rector --dry-run",
        "sniffer:php8": "./vendor/bin/phpcs -p ./src --standard=vendor/phpcompatibility/php-compatibility/PHPCompatibility --runtime-set testVersion 8.2"
    }
}
